{
  "name": "nextcloud",
  "version": "1.0.0",
  "private": true,
  "description": "Nextcloud Server",
  "keywords": [
    "nextcloud"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud/server.git"
  },
  "license": "AGPL-3.0-or-later",
  "author": "Nextcloud GmbH and Nextcloud contributors",
  "directories": {
    "lib": "lib",
    "test": "tests"
