msgid "To login page"
msgstr "Alla pagina di accesso"

#: lostpassword/templates/resetpassword.php:8
msgid "New password"
msgstr "Nuova password"

#: lostpassword/templates/resetpassword.php:11
msgid "Reset password"
msgstr "Ripristina la password"

